//my driver

public class PolygonDriver
{
 
	public static void main(String[] args) {
	    
	    
		RegularPolygon Polygon = new RegularPolygon(8,5.75);	//creates new polygon
		System.out.println("The polygon's area is "+Polygon.getArea()); // gets area of the polygon
		System.out.println("The polygon's perimeter is "+Polygon.getPerimeter()); // gets perimeter
		System.out.println("The polygon's vertex angle length is "+Polygon.getVertexAngle()); //gets vertex angle
		System.out.println("The radius of the inscribed circle is "+ Polygon.getiR()); // same
		System.out.println("The radius of the circumscribed circle is "+ Polygon.getcR()); // gets the value
}
}
