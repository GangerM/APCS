
public class NamedCow extends Cow
{
   private String myName;
   private String myType;
   private String mySound;
    public NamedCow(String name)
    {
       myName = name;
       myType = "cow";
       mySound = "moo";
    }
    public String getSound(){
        return mySound; 
    }

    public String getType(){
       return myType; 
    }

    public String getName(){
        return myName;
    }
}
