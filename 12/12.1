public class FunLoops
{   
   public int checkSquares(int x){
   int n = x*x;
   while(n > 0){
   if ((x*x) == sumOf(n)) return n;
   else n--;
   }
   return -1;
   }
   public int sumOf(int x){
    if (x == 1)
        return 1;
    else return x + sumOf(x-1);
    }
   public int MagicSquare(int n){
    int x = 1;
    while ( n > 0){
    if (checkSquares(x) > 0) return x*x;
    else{
    x++;
    }
    }
    return -1;
