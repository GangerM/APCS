public class Recursive
{
    public int multiply(int x, int y){
    if(x ==0 || y == 0)return 0; //base case for 0
    else if (y == 1) return x; // base case for 1
    else if (x ==1 ) return y; // base case for 1
    else return x + multiply(x, y-1); // recursive statement
    }
    
    public int fibNumber(int n)//method for recursion
    {if(n==1 || n==2) return 1;//base statements
     else if(n==0) return 0; // this one is a base statement as well
     else return fibNumber(n-1) + fibNumber(n-2);//recursive statement
    }
}
